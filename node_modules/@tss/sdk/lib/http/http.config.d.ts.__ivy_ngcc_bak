import { HttpParamInjectionDefinition } from './http-param-injection-definition';
import { RouteExpression } from './route-expression';
/**
 * Configuration class for storing global data caching settings.
 */
export declare class HttpConfig {
    readonly hasInjections: boolean;
    paramInjectionsEnabled: boolean;
    paramInjectionDefitions: HttpParamInjectionDefinition[];
    addOrUpdateParameterInjection(route: RouteExpression, paramName: string, paramValue: string): HttpParamInjectionDefinition;
    addParameterInjection(route: RouteExpression, paramName: string, paramValue: string): HttpParamInjectionDefinition;
    getInjectionDefinitions(route: string): HttpParamInjectionDefinition[];
    getFirstInjectionDefinition(route: string): HttpParamInjectionDefinition;
}
