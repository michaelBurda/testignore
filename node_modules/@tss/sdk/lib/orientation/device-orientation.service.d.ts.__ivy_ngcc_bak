import { Observable, Subject } from 'rxjs';
import { Orientation } from './orientation';
import { OrientationConfig } from './orientation.config';
/**
 * Geolocator service. Provides a single point of GPS interaction.
 */
export declare class DeviceOrientationService {
    private readonly config;
    readonly orientationChange: Observable<Orientation>;
    readonly headingChange: Observable<number>;
    readonly orientationChangeSource: Subject<Orientation>;
    readonly headingChangeSource: Subject<number>;
    private internalDeviceNeedsCalibrated;
    readonly deviceNeedsCalibrated: boolean;
    constructor(config: OrientationConfig);
    mockOrientation(orientation: Orientation): this;
    mockHeading(heading: number): this;
    private emitOrientationChange;
}
