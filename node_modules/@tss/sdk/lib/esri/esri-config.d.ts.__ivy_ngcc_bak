export declare type ArcgisQueryMethodStrategy = 'prefer-get' | 'force-get' | 'force-post';
/**
 * Configuration class for storing global ESRI-related settings.
 */
export declare class EsriConfig {
    arcgis: {
        geometryUrl?: string;
    };
    arcgisQueryMethodStrategy: ArcgisQueryMethodStrategy;
    arcgisMaxWhereLengthGetThreshold: number;
    arcgisPortalUrl: string;
    arcgisPortalClientId: string;
    readonly hasGeometryUrl: boolean;
    readonly arcGisGeometryUrl: string;
}
