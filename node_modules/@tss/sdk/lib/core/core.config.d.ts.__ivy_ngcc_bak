import { LogLevel } from './logging/log-level';
import { AutoConfigData } from './auto-config-data';
import { MeasurementUnit } from './units/measurement-unit';
/**
 * A simple settings/config class for storing key/value pairs with persistence.
 */
export declare class CoreConfig {
    static readonly moduleConfigs: AutoConfigData[];
    autoConfig: boolean;
    logEnabled: boolean;
    logLevel: LogLevel;
    defaultNotifyInfoMsg: string;
    defaultNotifyWarnMsg: string;
    defaultNotifyErrorMsg: string;
    defaultNotifySuccessMsg: string;
    defaultNotifyWaitMsg: string;
    measurementUnit: MeasurementUnit;
}
