import { Observable, Subject } from 'rxjs';
import { Motion } from './motion';
import { MotionConfig } from './motion.config';
/**
 * Geolocator service. Provides a single point of motion API.
 */
export declare class DeviceMotionService {
    private readonly config;
    readonly motionChange: Observable<Motion>;
    readonly motionChangeSource: Subject<Motion>;
    constructor(config: MotionConfig);
    mockMotion(motion: Motion): this;
    private emitMotionChange;
}
