import { ResourceConfig } from '../core/resource.config';
/**
 * Configuration class for storing global ESRI-related settings.
 */
export declare class DataSourceConfig extends ResourceConfig {
    dataSourcesEndpoint: string;
    tablesEndpoint: string;
    columnsEndpoint: string;
    tableActionEndpoint: string;
    editDataEndpoint: string;
    applyEditsEndpoint: string;
    attachmentsEndpoint: string;
    skipColumnQuotes: boolean;
}
