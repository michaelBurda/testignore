import { HttpClient, HttpParams } from '@angular/common/http';
import { Observable } from 'rxjs';
import { Attachment } from './attachment';
import { DataSourceConfig } from '../data-source-config';
import { BaseResourceService } from '../../core/resource/base-resource.service';
export declare class AttachmentService extends BaseResourceService<Attachment> {
    protected http: HttpClient;
    constructor(http: HttpClient, dsConfig: DataSourceConfig);
    create(attachment: Attachment, routeTokens?: any, params?: HttpParams): Observable<Attachment>;
    download(attachmentId: string, routeTokens?: any, params?: HttpParams): void;
}
