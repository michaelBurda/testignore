import { ResourceConfig } from '../core/resource.config';
import { AppProfile } from './app-profile';
export declare const DEFAULT_APP_ID = "all";
export declare const DEFAULT_PROFILE_ID = "default";
/**
 * Application related configuration options. These settings are used to
 * drive all general "global" application related behaviour. The settings can be
 * overridden directly in the code, application module, or from the TDS server.
 */
export declare class AppConfig extends ResourceConfig {
    appId: string;
    queryProfile: boolean;
    profileId: string;
    profile: AppProfile;
    defaultProfileId: string;
    allowQueryParamInjections: boolean;
    continueOnAppBootstrapError: boolean;
    appSettingsEndpoint: string;
    appProfileEndpoint: string;
    pingEndpoint: string;
    statusEndpoint: string;
}
