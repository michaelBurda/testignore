import { HttpClient, HttpParams } from '@angular/common/http';
import { Observable } from 'rxjs';
import { BaseResourceService } from '../../core/resource/base-resource.service';
import { AppSetting } from './app-setting';
import { AppConfig } from '../app.config';
export declare class AppSettingService extends BaseResourceService<AppSetting> {
    protected http: HttpClient;
    constructor(http: HttpClient, config: AppConfig);
    /**
     * Retrieves a list of application settings.
     */
    queryAllApps(routeTokens?: any, params?: HttpParams): Observable<AppSetting[]>;
}
