import { HttpClient, HttpParams } from '@angular/common/http';
import { Observable } from 'rxjs';
import { AppConfig } from './app.config';
import { BaseEmptyResourceService } from '../core/resource/base-empty-resource.service';
export interface ApiStatus {
    up: boolean;
    down: boolean;
    configStoreUp: boolean;
    configStoreDown: boolean;
    upSince: Date;
    daysUp: number;
    appCount: number;
}
export declare class ApiService extends BaseEmptyResourceService<ApiStatus> {
    private appConfig;
    constructor(http: HttpClient, appConfig: AppConfig);
    ping(routeTokens?: any, params?: HttpParams): Observable<void>;
    status(routeTokens?: any, params?: HttpParams): Observable<ApiStatus>;
}
