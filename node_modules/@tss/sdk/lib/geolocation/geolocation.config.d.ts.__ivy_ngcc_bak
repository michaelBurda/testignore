/**
 * Configuration class for storing geolocation-related settings.
 */
export declare class GeolocationConfig {
    gpsDisplayDecimalPlaces: number;
    gpsWarnableAccuracyMeters: number;
    gpsRequiredAccuracyMeters: number;
    gpsWarnableDilutionOfPrecision: number;
    gpsRequiredDilutionOfPrecision: number;
    gpsEnableHighAccuracy: boolean;
    gpsNoAccuracyReading: string;
    allowGpsMocking: boolean;
    emitDuplicateGpsPositions: boolean;
    gpsPositionDuplicatePrecision: number;
    gpsAntennaHeight: number;
    adjustForGpsAntennaHeight: boolean;
    calculateMissingPositionValues: boolean;
    geolocationGeolocatorName: string;
    geolocationGeolocatorDescription: string;
}
