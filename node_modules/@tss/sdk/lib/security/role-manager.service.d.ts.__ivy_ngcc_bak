import { SecurityConfig } from './security-config';
export declare class RoleManagerService {
    private config;
    private readonly roles;
    private readonly roleSplitterExpr;
    constructor(config: SecurityConfig);
    readonly enabled: boolean;
    readonly roleCount: number;
    getRoles(): string[];
    setRoles(roles: string | string[] | number | number[]): void;
    addRoles(roles: string | string[] | number | number[]): void;
    addRole(role: string | number): void;
    clearRoles(): void;
    hasRole(role: string | number): boolean;
    hasRoles(roles: string | number): boolean;
    hasAllRoles(roles: string | string[] | number | number[]): boolean;
    hasAnyRoles(roles: string | string[] | number | number[]): boolean;
    private forRealHasRole;
    private toStringArray;
    private toArray;
}
